public class DealerIncentiveCalculate_transrecall_ba implements Database.Batchable<SObject>, Database.Stateful {

	public Date processDate;
	public String query;
	public Boolean isNextBatch;

	public DealerIncentiveCalculate_transrecall_ba() {
		this(true);
	}

	public DealerIncentiveCalculate_transrecall_ba(Boolean isNextBatch) {
		this.isNextBatch = isNextBatch;
	}

	public Database.QueryLocator start(Database.BatchableContext BC) {
		if(processDate == null) processDate = Date.today().addDays(-1);

		if(String.isEmpty(query)) {
			return Database.getQueryLocator([
					SELECT Id, Name,
							AssetId__c,
							AssetId__r.FirstDeliveryDate__c,
							AssetId__r.Name,
							SalesDocNo__c,
							SalesDocItem__c,
							OrderSellIn__c,
							Dealer__c,
							Dealer__r.IsSalesIQComplete__c,
							Dealer__r.DealerDivision__c
					FROM Delivery__c
					WHERE DateOfLoading__c = YESTERDAY
					AND DeliveryType__c = '전수배반품'
			]);
		} else {
			return Database.getQueryLocator(query);
		}

	}

	public void execute(Database.BatchableContext BC, List<Delivery__c> scope) {
		DealerIncentiveCalculate_com_ba.DealerIncentiveCalculate_transrecall_ba(scope, processDate);
	}

	public void finish(Database.BatchableContext BC) {
		if(isNextBatch) Database.executeBatch(new DealerIncentiveCalculate_recall_ba(), 200);
	}

