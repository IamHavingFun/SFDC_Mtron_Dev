public without sharing class OrderLoanApprovalCancel {

	/**
	 * Wrapper class for InitData
	 */
	public class InitData extends I2ComClass.InitData {
		@AuraEnabled public Order__c order {get;set;}
		@AuraEnabled public Boolean isProcess {get;set;}
		@AuraEnabled public String processMessage {get;set;}
	}

	/**
	 * Init method
	 *
	 * @return I2ComClass.InitData
	 */
	@AuraEnabled
	public static InitData init(final String recordId) {
		InitData initData = new InitData();
		initData.order = [
				SELECT Id, Name, Status__c, ErpCMGST__c, LoanApprovalStatus__c, ErpZLNNO__c, SalesDocNo__c, PurposeOfOrder__c, ErpLRESN_T__c, ErpLPLAN_T__c,ErpLFDBK_T__c
				FROM Order__c
				WHERE Id = :recordId
		];

		initData.isProcess = false;
		initData.processMessage = '';
		if(initData.order.Status__c == '본사 여신 검토' &&
			String.isEmpty(initData.order.ErpZLNNO__c) == false )
		{
			initData.isProcess = true;
		}
		if(initData.isProcess == false) {
			initData.processMessage = '추가여신승인요청이 진행 중이거나 완료되었을 때에만 취소가 가능합니다.';
		}

		return initData;
	}

	/**
	 * Init method
	 *
	 * @return I2ComClass.InitData
	 */
	@AuraEnabled
	public static ReturnData  save(final Order__c order) {

		ReturnData rd = new ReturnData();
		rd.order = order;

		String zflag = 'D';     // ZFLAG = 'I' 생성, ZFLAG = 'C' 변경, ZFLAG = 'D' 삭제,
		IF_KR_SE_ERP_0040.OutputDIH result = call_IF_KR_SE_ERP_0040_Cancel(order, zflag);
		System.debug(JSON.serializePretty(result));
		if(result.response.header.ifResult == 'S') {
			// 성공
			rd.returnCode = '00';
			rd.returnMsg = '추가여신 문서 생성 I/F 설공';

			List<ProcessInstanceWorkitem> piwiList = [SELECT Id, ProcessInstanceId, ProcessInstance.TargetObjectId FROM ProcessInstanceWorkitem WHERE ProcessInstance.TargetObjectId =: order.Id];
			if(piwiList.isEmpty() == false) {
				Approval.ProcessWorkitemRequest req = new Approval.ProcessWorkitemRequest();
				req.setAction('Removed');
				req.setWorkitemId(piwiList.get(0).Id);
				Approval.process(req, false);
			}
		} else {
			// 실패
			rd.returnCode = '99';
			rd.returnMsg = '[ERP 메세지] :'+ result.response.header.ifFailMsg;
		}

		if(rd.returnCode == '00') {
			order.LoanApprovalStatus__c = '';
			order.ErpZLNNO__c = '';
			update order;
		}

		return rd;
	}



	/**
	 * 주문 추가 여신 승인 취소 호출.
	 *
	 * @param recordId : Order__c 의 RecordId
	 */
	@AuraEnabled
	public static IF_KR_SE_ERP_0040.OutputDIH call_IF_KR_SE_ERP_0040_Cancel(final Order__c order, String zflag) {

		User user = [SELECT Id, EmployeeNumber FROM User WHERE Id = :UserInfo.getUserId()];

		OrderLineitem__c oli = [SELECT Id FROM OrderLineitem__c WHERE OrderId__c = :order.Id LIMIT 1];

		IF_KR_SE_ERP_0040 instance = IF_KR_SE_ERP_0040.getInstance();
		// 서비스 실행
		IF_KR_SE_ERP_0040.InputDIH input = new IF_KR_SE_ERP_0040.InputDIH();
		IF_KR_SE_ERP_0040.INPUT_T_DATA tData = new IF_KR_SE_ERP_0040.INPUT_T_DATA();

		tData.SE_REQ_HEADER_NO   = order.Id;
		tData.ZFLAG              = zflag;
		tData.PERNR              = user.EmployeeNumber;
		tData.ZLNNO              = order.ErpZLNNO__c;
		tData.SE_REQ_ITEM_NO     = order.Id;
		tData.SE_REQ_ITEM        = oli.Id;
		tData.VBELN              = order.SalesDocNo__c;
		tData.POSNR              = '000010';
		tData.RQIND              = order.PurposeOfOrder__c == '재고보충' ? 'F' : 'G';
		tData.REMARK             = '';

		// 연체 사유
		IF_KR_SE_ERP_0040.DETAIL_DATA detail = new IF_KR_SE_ERP_0040.DETAIL_DATA();
		detail.SE_REQ_TEXT_NO    = order.Id;
		detail.TDID              = 'Z001';
		detail.ZSEQ              = '0001';
		detail.TDLINE            = order.ErpLRESN_T__c;
		tData.Detail.add(detail);

		// 수금계획 : ErpLPLAN_T__c
		IF_KR_SE_ERP_0040.DETAIL_DATA detail2 = new IF_KR_SE_ERP_0040.DETAIL_DATA();
		detail2.SE_REQ_TEXT_NO    = order.Id;
		detail2.TDID              = 'Z002';
		detail2.ZSEQ              = '0002';
		detail2.TDLINE            = order.ErpLPLAN_T__c;
		tData.Detail.add(detail2);

		// 담당자 코멘트
		IF_KR_SE_ERP_0040.DETAIL_DATA detail3 = new IF_KR_SE_ERP_0040.DETAIL_DATA();
		detail2.SE_REQ_TEXT_NO    = order.Id;
		detail2.TDID              = 'Z004';
		detail2.ZSEQ              = '0003';
		detail2.TDLINE            = order.ErpLFDBK_T__c;
		tData.Detail.add(detail3);

		input.request.payload.t_data.add(tData);

		System.debug('result input : '+ JSON.serialize(input));

		return instance.execute(input);

	}

	public class ReturnData {
		@AuraEnabled public Order__c order {get;set;}
		@AuraEnabled public String returnCode  {get;set;}
		@AuraEnabled public String returnMsg  {get;set;}
	}


