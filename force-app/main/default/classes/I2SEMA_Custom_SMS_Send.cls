public with sharing class I2SEMA_Custom_SMS_Send {

	@AuraEnabled
	public static SmsVO init() {
		SmsVO smsVO = new SmsVO();
		List<Account> targetPhoneSets = getTargetMobiles();

		smsVO.noti = new Notification__c();
		smsVO.targetCount = targetPhoneSets.size();

		return smsVO;
	}

	public class SmsVO {
		@AuraEnabled public Notification__c noti {get;set;}
		@AuraEnabled public Decimal targetCount {get;set;}
	}

	private static List<Account> getTargetMobiles() {
		/**
		 * 대상 조건
		 * 1. 자산 유형 : 실판매
		 * 2. 고객 레코드 유형 : 개인 고객
		 * 3. 고객 이름 != UnKnown
		 * 4. 개인 정보 동의 = TRUE
		 * 5. 고객 휴대폰번호가 '01' 로 시작하는건 (일부 집 전화번호가 있음)
		*/
		List<Asset> baseTargets =
		[
				SELECT Name, Account.Name, Account.PersonMobilePhone, Account.IsCUPI__pc,
						Dealer__r.Name, Dealer__r.Phone, Dealer__r.fm_Mobile__c
				FROM Asset
				WHERE SalesDate__c != NULL
				AND InventoryType__c = '실판매'
				AND Account.RecordType.DeveloperName  = 'PersonAccount'
				AND Account.Name != 'UnKnown'
				AND Account.IsCUPI__pc = TRUE
				AND Account.PersonMobilePhone LIKE '01%'
		];
		// 1. 전화번호가 같은 중복 자산 고객은 제외 처리 한다.
		List<Asset> existDupTargets = new List<Asset>();
		Set<String> phones = new Set<String>();
		Map<String, Asset> existDupAssetMap = new Map<String, Asset>();
		Set<String> dealerPhoneSets = new Set<String>();
		for(Asset r : baseTargets) {
			dealerPhoneSets.add(r.Dealer__r.fm_Mobile__c);
			if(!existDupAssetMap.containsKey(r.Account.PersonMobilePhone)) {
				// 자산 고객의 전화번호가 중복이 있으면 제외 한다.
				existDupAssetMap.put(r.Account.PersonMobilePhone, r);
				phones.add(r.Account.PersonMobilePhone);
			}
		}

		// 2. 전체 전화번호중 중복이 있으면 제외 처리 한다.
		List<Account> accounts = [
				SELECT Id, Name, PersonMobilePhone
				FROM Account
				WHERE PersonMobilePhone IN : phones
				AND RecordType.DeveloperName = 'PersonAccount'
		];
		Map<String, Account> accountMap = new Map<String, Account>();
		Set<String> dupPhones = new Set<String>();
		for(Account r : accounts) {
			if(accountMap.containsKey(r.PersonMobilePhone)) {
				// 중복 있으면 없앤다.
				dupPhones.add(r.PersonMobilePhone);
			} else {
				// 대리점주 전화번호가 있으면 제외 처리 한다.
				if(!dealerPhoneSets.contains(r.PersonMobilePhone)) {
					accountMap.put(r.PersonMobilePhone, r);
				}
			}
		}

		System.debug('발송 대상 건수 : ' + accountMap.size());
		return accountMap.values();
	}

	@AuraEnabled
	public static void getTargetCount(String type) {
		List<Account> targetPhoneSets = getTargetMobiles();
		System.debug('발송 대상 건수 : ' + targetPhoneSets.size());
	}

	@AuraEnabled
	public static NotificationGroup__c send(String templateId) {

		i2SEMA_TriggerHandler.bypass('i2SEMA_Notification_tr');

		NotificationGroup__c notiGroup = new NotificationGroup__c();
		notiGroup.TargetId__c = null;
		notiGroup.IF_Status__c = 'Pending';
		notiGroup.SendType__c = '고객센터 알림';
		insert notiGroup;
		System.debug('그룹 아이디 : ' + notiGroup.Id);

		NotificationTemplate__c template = [SELECT Id
				,Channel__c
				,Title__c
				,Content__c
				,LABEL1__c
				,LABEL2__c
				,LABEL3__c
				,LABEL4__c
				,LABEL5__c
				,Type__c
				,Ars080Phone__c
		FROM NotificationTemplate__c
		WHERE Id =:templateId];

		List<Account> targetPhoneSets = getTargetMobiles();
		List<Notification__c> notifications = new List<Notification__c>();
		for(Account target : targetPhoneSets) {
			Notification__c noti = new Notification__c();
			noti.NotificationGroup__c = notiGroup.Id;
			noti.TargetId__c = null;
			noti.Phone__c = target.PersonMobilePhone;
			noti.NotificationTemplate__c = templateId;
			noti.Lead__c = null;
			noti.Account__c = target.Id;
			noti.User__c = null;
			noti.Contact__c = null;
			noti.LABEL1__c = null;
			noti.LABEL2__c = null;
			noti.LABEL3__c = null;
			noti.LABEL4__c = null;
			noti.LABEL5__c = null;
			noti.IsSendingReservation__c = false;
			noti.SendingTime__c = Datetime.now();
			noti.msg_status__c = null;
			noti.Channel__c = template.Channel__c;
			noti.Title__c = template.Title__c;
			noti.RawContents__c = template.Content__c;
			noti.Content__c = template.Content__c;
			notifications.add(noti);
		}
		insert notifications;

		i2SEMA_TriggerHandler.clearBypass('i2SEMA_Notification_tr');

		// 배치 호출 처리.
		I2SEMA_Custom_SMS_Send_ba batch = new I2SEMA_Custom_SMS_Send_ba();
		batch.notificationGroupId = notiGroup.Id;
		Database.executeBatch(batch, 200);

		NotificationGroup__c returnNotiGroup = [SELECT Id, Name, SendType__c FROM NotificationGroup__c WHERE Id = :notiGroup.Id];

		return returnNotiGroup;

	}

