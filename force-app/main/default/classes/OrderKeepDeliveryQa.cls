public without sharing class OrderKeepDeliveryQa {

	/**
	 * 초기 거래 데이터
	 */
	public class InitData extends I2ComClass.InitData {
		@AuraEnabled public OrderKeep__c orderKeep {get;set;}
		@AuraEnabled public Boolean isProcess {get;set;}
		@AuraEnabled public String validMessage {get;set;}
	}

	/**
	 * 초기 거래
	 * @return InitData
	 */
	@AuraEnabled(Cacheable=true)
	public static InitData init(String recordId) {
		InitData initData = new InitData();

		initData.orderKeep = [
				SELECT Id, Status__c, SalesDocNo__c, IsOutAsset__c, KeepAsset__r.Name, KeepAsset__r.BatchNo__c, KeepAsset__r.ErpLGORT__c,
						Order__r.Division__c, Order__r.VSTEL__c
				FROM OrderKeep__c
				WHERE Id =: recordId ];

		initData.isProcess = true;

		Set<String> checkProfiles = new Set<String>{
				'시스템 관리자',
				'시스템 관리자(C)'
		};
		List<User> targetUsers = [
				SELECT
						Id,
						UserName,
						UserRole.Name,
						Profile.Name
				FROM User
				WHERE (
						UserRole.DeveloperName = 'DomesticSalesHQ'
						OR Profile.Name IN : checkProfiles
				)
				AND Id = :UserInfo.getUserId()
		];

		if(initData.orderKeep.Status__c == '보관출고') {
			initData.isProcess = false;
			initData.validMessage = '이미 보관출고가 완료 되었습니다.';
			return initData;
		} else if(initData.orderKeep.Status__c == '출하지시') {
			initData.isProcess = false;
			initData.validMessage = '이미 출하지시 처리가 되었습니다.';
			return initData;
		} else if(initData.orderKeep.Status__c == '주문입력') {
			initData.isProcess = false;
			initData.validMessage = '보관주문의 상태가 [보관처리]가 되어, 보관처리가 완료된 경우만 보관주문의 출하 지시 (보관출고)가 가능합니다';
			return initData;
		}

		// 재고보충에 전주 공장 출하이면 : 국내영업본부 + 혁신파트(시스템관리자) 만 처리 가능.
		if(
				initData.orderKeep.Order__r.Division__c == '공장주문' &&
						(initData.orderKeep.Order__r.VSTEL__c == 'M620' || String.isEmpty(initData.orderKeep.Order__r.VSTEL__c) == true))
		{
			if(initData.isProcess == true && targetUsers.isEmpty() == true) {
				initData.isProcess = false;
				initData.validMessage = '전주공장(M620)에서의 출하는 국내영업본부 혹은 시스템 관리자만 진행 가능합니다.';
			}
		}

		return initData;
	}


	/**
	 * 보관처리의 출고지시
	 *
	 * @param recordId : Order__c 의 RecordId
	 * @param deliveryRecord : 저장할 출하 레코드
	 */
	@AuraEnabled
	public static DeliveryVO set0038KeepOut(final OrderKeep__c orderKeep) {
		DeliveryVO vo = new DeliveryVO();
		vo.orderKeep = orderKeep;
		IF_KR_SE_ERP_0038.OutputDIH ifResult = null;
		String WADAT = String.valueOf(system.now().format('yyyyMMdd'));
		String LGORT = orderKeep.KeepAsset__r.ErpLGORT__c;

		System.debug(JSON.serializePretty(orderKeep));

		ifResult = OrderService.IF_KR_SE_ERP_0038(orderKeep.SalesDocNo__c, WADAT, orderKeep.KeepAsset__r.BatchNo__c, LGORT );
		if(orderKeep.IsOutAsset__c == false) {
			if ('S'.equals(ifResult.response.header.ifResult)) {
				vo.isSuccess = true;
                orderKeep.IsOutAsset__c = true;
                orderKeep.Status__c = '출고지시';
                orderKeep.KeepDeliveryDate__c = Date.today();
                update orderKeep;
			} else {
				vo.isSuccess = false;
				vo.message = ifResult.response.header.ifFailMsg;
			}
		} else {
			vo.isSuccess = true;
		}
		return vo;
	}

	public class DeliveryVO {
		@AuraEnabled public OrderKeep__c orderKeep {get;set;}
		@AuraEnabled public Boolean isSuccess {get;set;}
		@AuraEnabled public String message {get;set;}
	}


