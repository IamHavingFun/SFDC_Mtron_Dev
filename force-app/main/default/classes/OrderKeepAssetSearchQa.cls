public with sharing class OrderKeepAssetSearchQa {

	public class InitData extends I2ComClass.InitData {
		@AuraEnabled public OrderKeep__c orderKeep {get;set;}
		@AuraEnabled public Boolean  isButtonSave {get;set;}
		@AuraEnabled public String   pageComment {get;set;}
	}

	@AuraEnabled
	public static I2ComClass.InitData init(String recordId) {
		InitData initData = new InitData();
		initData.orderKeep = [
				SELECT Id, Order__r.OrderProduct__c, Order__r.OrderProduct__r.Name, KeepAsset__c, IsOutAsset__c
				FROM OrderKeep__c
				WHERE Id = :recordId
		];
		// 보관 재고 처리 대상 여부 확인
		// 1. 판매문서번호 O
		// 2. 주문 상태 = 출하
		// 3. 레코드 타입 = 농협관납 주문건만 해당.
		initData.isButtonSave = true;
		if(initData.orderKeep.IsOutAsset__c == true) {
			initData.isButtonSave = false;
			initData.pageComment = '출하 지시된 주문의 보관 자산은 변경할 수 없습니다.';
		}
		return initData;
	}

	@AuraEnabled
	public static I2ComClass.ResData getSearch(String reqData, Integer pageSize, Integer pageNumber){

		String whereClause = 'WHERE InventoryType__c = \'공장재고(가용)\' ';
		Map<String, String> paramMap = I2ComHelper.gfnCheckCondition(reqData);

		if(I2ComHelper.gfnHasCondition(paramMap, 'assetName')) {
			// 제품 정보가 있는 경우.
			whereClause += ' AND Name LIKE \'%' + paramMap.get('assetName') + '%\'' + '\n';
		}
		if(I2ComHelper.gfnHasCondition(paramMap, 'productId')) {
			// 제품 정보가 있는 경우.
			whereClause += ' AND Product2Id = \'' + paramMap.get('productId') + '\'' + '\n';
		}
		whereClause = I2ComHelper.gfnRefineCondition(whereClause);

		I2ComClass.ResData resData = I2ComHelper.gfnQuery(
				'Id, Name, InventoryType__c, Product2.Name, Product2.ProductCode, Product2.ProductDivision__c, (SELECT Id, Order__r.Name FROM OrderKeep_KeepAsset__r) ',
				'Asset',
				whereClause,
				null,
				pageSize,
				pageNumber
		);

		return resData;
	}

	/**
	 * @description 수수료 계산 결과 리턴 하는 method.
	 *
	 * @param wrapperData : OrderNewQa.WrapperData
	 *
	 * @return OrderNewQa.WrapperData
	 */
	@AuraEnabled
	public static void save(OrderKeep__c orderKeep) {
        update orderKeep;
	}


