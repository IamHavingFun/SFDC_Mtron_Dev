<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes"
                description="tableauIframeConnector">

    <aura:attribute name="tsname" type="String" default="SE_Dashboard" required="true" description="대시보드 이름"/>

    <aura:attribute name="biLink_total" type="String" default="" required="true" description="전체 BI 대쉬보드 url"/>
    <aura:attribute name="biLink_1" type="String" default="" required="true" description="전체 BI 대쉬보드 url"/>
    <aura:attribute name="biLink_2" type="String" default="" required="true" description="전체 BI 대쉬보드 url"/>
