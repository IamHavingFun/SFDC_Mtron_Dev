    <aura:attribute name="biLink_7" type="String" default="" required="true" description="전체 BI 대쉬보드 url"/>
    <aura:attribute name="biLink_8" type="String" default="" required="true" description="전체 BI 대쉬보드 url"/>

    <article class="mapArea">
        <div class="mapBox">
            <div class="mapTit">
                <ul>
                    <a href="{#v.biLink_total}" target="_blank"><li>전체 <div class="map_color1"></div></li></a>
                    <a href="{#v.biLink_1}" target="_blank"><li>경기 <div class="map_color2"></div></li></a>
                    <a href="{#v.biLink_2}" target="_blank"><li>강원 <div class="map_color3"></div></li></a>
                    <a href="{#v.biLink_3}" target="_blank"><li>충북 <div class="map_color4"></div></li></a>
                    <a href="{#v.biLink_4}" target="_blank"><li>충남 <div class="map_color5"></div></li></a>
                    <a href="{#v.biLink_5}" target="_blank"><li>전북 <div class="map_color6"></div></li></a>
                    <a href="{#v.biLink_6}" target="_blank"><li>전남 <div class="map_color7"></div></li></a>
                    <a href="{#v.biLink_7}" target="_blank"><li>경북 <div class="map_color8"></div></li></a>
                    <a href="{#v.biLink_8}" target="_blank"><li>경남 <div class="map_color9"></div></li></a>
                </ul>
            </div>
            <div class="">
                <img src="/resource/img_map" style="height:540px"/>
            </div>
        </div>
    </article>
