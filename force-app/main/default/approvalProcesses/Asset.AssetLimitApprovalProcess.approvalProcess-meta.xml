    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <description>영업팀장 결재</description>
        <label>팀장 승인</label>
        <name>AssetManagerApprovalProcess</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <description>영업담당 전결</description>
        <label>임원 승인</label>
        <name>ApprovalProcess2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>전수배 2회 제한 해제 요청 프로세스</description>
    <emailTemplate>ApprovalFolder/ApprovalAssetEmailTemplate</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(
				ISPICKVAL(InventoryType__c, &quot;대리점재고&quot;),
				ISPICKVAL(AdditionalReuqestStatus__c ,&quot;&quot;),
				TransferCount__c = 1
			)</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>AddTransferApproveTrue</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>ApprovedActionFieldUpdate</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>RequestActionFieldUpdate</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>전수배 제한 해제 승인 프로세스</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>RejectActionFieldUpdate</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
